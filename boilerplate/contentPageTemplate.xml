<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
    <head xmlns="http://www.w3.org/1999/xhtml">
        <title><?siteTitle?></title>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="stylesheet" href="{?cssFile}" />
        <link rel="icon" href="{?faviconIco}" sizes="48x48" />
        <link rel="icon" href="{?faviconSvg}" sizes="any" type="image/svg+xml" />
        <link rel="manifest" href="{?siteManifest}" />

    </head>
    <body id="top">
        <header data-state="closed" id="site-header" class="open">
            <span class="internalTitle"><?splashTitle?></span>
            <span class="internalSubTitle"><?splashSubtitle?></span>
            <span class="subheaderInternal"><?splashWhereWhen?></span>
            <div id="mobile-nav-banner" data-state="closed">
                <a href="{?homeUrl}">
                    <img class="site-logo-mobile" src="{?mobileLogo}"
                         alt="{?LogoAlt}" />
                </a>
           </div>
            <div class="nav-wrapper">
                <?bilingualSwitcher?>
                <nav class="main-menu" id="site-navigation" aria-expanded="true">
                    <ul class="clicky-menu no-js" role="menu">
                        <?navigationMenu?>
                    </ul>
                    <div class="mobile-nav-footer">
                        <img class="mobile-footer-logo" src="{?mobileFooterLogo}"
                            width="50" height="50" alt="" />
                    </div>
                </nav>

                <a href="{?homeUrl}">
                    <img class="site-logo" src="{?siteLogo}"
                        alt="{?LogoAlt}" />
                </a>


            </div>
   

        </header>
        <a id="content"></a>
        <!-- NOTE: This processing-instruction will be replaced by the document 
             content. -->
        <?docContent?>
        
        <footer>
            
            <div class="site-specific-footer">
                <img class="site-footer-logo" src="{?footerLogo}" alt="{?LogoAlt}" />
            </div>
            
            <div class="footer-info">
                <div class="copyright"><?copyrightText?></div>
            </div>
            <div class="citation"><?citationText?></div>
            <div class="logos">
                <a class="uvic-logo-internal" href="{?uvicUrl}">
                    <img src="{?uvicLogo}" alt="{?LogoAlt}" />
                </a>
            </div>
            <div class="acknowledgements"><?acknowledgementsText?></div>
            <div id="buildInfo"><!-- Build info will go here. --></div>
        </footer>
    </body>

</html>
